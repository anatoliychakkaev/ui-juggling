<div class="page-header"><h1>Edit <%- modelName %></h1></div>

<% formFor(model, {action: pathTo.model(modelName, model), method: 'PUT', id: "model-form", class: 'form-horizontal'}, function (form) { %>
    <%- partial('models/form.ejs', {locals: {form: form}}) %>
    <div class="form-actions">
        <%- form.submit('<i class="icon-ok icon-white"></i> Update ' + modelName, {class: 'btn btn-primary'}) %> or
        <%- linkTo('Cancel', pathTo.model(modelName, model), {class: 'btn'}) %>
    </div>
<% });%>

