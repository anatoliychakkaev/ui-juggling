<div class="page-header">
    <h1>Details of <%- modelName %></h1>
</div>

<table class="table table-bordered">
    <tbody>
        <% Object.keys(properties).forEach(function (p) { %>
        <tr>
            <th><%= p %></th>
            <td><%- stringify(model[p]) %></td>
        </tr>
        <% }) %>
    </tbody>
</table>

<div class="well">
    <%- link_to('<i class="icon-edit icon-white"></i> Edit', pathTo.editModel(modelName, model), {class: 'btn btn-primary'}) %>
    <%- link_to('<i class="icon-remove icon-white"></i> Delete', pathTo.model(modelName, model), {class: 'btn btn-danger', method: 'delete', remote: true, jsonp: '(function (u) { location.href = u; })'}) %>
    or
    <%- link_to('Back to index', pathTo.models(modelName)) %>
</div>
